<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ABCD_OnlineMall</title>
    <link rel="icon" href="data:image/jpeg;base64," type="image/icon type">
    <link href="~/client/css/main/base.css" rel="stylesheet" />
    <link href="~/client/css/main/header.css" rel="stylesheet" />
    <link href="~/client/css/main/footer.css" rel="stylesheet" />
    <link href="~/client/font/fontawesome-free-5.15.2-web/css/all.min.css" rel="stylesheet" />
    <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.js"
            integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
            crossorigin="anonymous"></script>
    @RenderSection("mapfloorcss", required: false)
    @RenderSection("homecss", required: false)
    @RenderSection("homejs", required: false)
    @RenderSection("onload", required: false)
    @RenderSection("storecss", required: false)
    @RenderSection("productcss", required: false)
    @RenderSection("productdetailcss", required: false)
    @RenderSection("detailBrandcss", required: false)
    @RenderSection("feedbackcss", required: false)
    @RenderSection("detailcss", required: false)
    @RenderSection("headerjs", required: false)
    @RenderSection("scripts", required: false)

</head>
<body>
    <header class="header">
        <div class="wrap">
            <div class="header-nav">
                <a href="/" class="header-logo text-logo" style="text-decoration: none">
                    <div class="text-logo-left">ABCD.</div>
                    <div class="text-logo-right">maLL</div>
                </a>
                <ul class="header-nav-list">
                    <li class="header-nav-item">
                        <a href="/Home/Index" class="header-nav-item__link">
                            Home
                        </a>
                    </li>
                    <li class="header-nav-item header-nav-dropdown-menu">
                        <a href="/Stores/Index" class="header-nav-item__link">
                            Stores
                        </a>

                    </li>
                    <li class="header-nav-item">
                        <a href="/Brands/Index" class="header-nav-item__link">
                            Brands
                        </a>
                    </li>
                    <li class="header-nav-item">
                        <a href="/Products/Index" class="header-nav-item__link">
                            Products
                        </a>
                    </li>
                    <li class="header-nav-item">
                        <a href="/cinemas/indexclient" class="header-nav-item__link" target="_blank">
                            Cinemas
                        </a>
                    </li>
                </ul>

                <div class="header-nav-left">
                    <a class="header-nav-left-item" href="/FeedBacks/Create">
                        <i class="far fa-comments header-nav-left-time-icon"></i>
                        <span class="header-nav-left-time-text">FeedBack</span>
                    </a>
                    <a class="header-nav-left-item" href="/plots/index">
                        <i class="fas fa-building header-nav-left-map-icon"></i>
                        <span class="header-nav-left-map-text">Map floors</span>
                    </a>
                    <a class="header-nav-left-item" href="/Contact/Index">
                        <i class="far fa-address-book header-nav-left-contact-icon"></i>
                        <span class="header-nav-left-contact-text">Contact</span>
                    </a>
                    <a class="header-nav-left-item" href="/admin/Login">
                        <i class="fas fa-cog header-nav-left-contact-icon"></i>
                        <span class="header-nav-left-contact-text">Settings</span>
                    </a>
                </div>
            </div>
        </div>
        <div id="google_translate_element"></div>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer>
        <div class="footer">
            <div class="footer-wrap">
                <div class="grid footer-border">
                    <div class="col-3">
                        <div class="logo-footer">
                            <a href="/Home/Index" class="logo-footer-link">
                                <span class="logo-footer-left">ABCD.</span>
                                <span class="logo-footer-right">maLL</span>
                            </a>
                            <h2 class="footer-heading-"></h2>
                        </div>
                    </div>
                    <div class="col-3">
                        <h1 class="footer-heading">Galary</h1>
                        <ul class="footer-list">
                            <li class="footer-item">
                                <a href="/Cinemas/IndexClient" class="footer-item-link" target="_blank">
                                    Movies
                                </a>
                            </li>
                            <li class="footer-item">
                                <a href="/Stores/IndexClothing" class="footer-item-link">
                                    Clothings
                                </a>
                            </li>
                            <li class="footer-item">
                                <a href="/Stores/IndexFood" class="footer-item-link">
                                    Foods
                                </a>
                            </li>
                            <li class="footer-item">
                                <a href="/Stores/IndexOther" class="footer-item-link">
                                    Others
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-3">
                        <h1 class="footer-heading">Support</h1>
                        <ul class="footer-list">
                            <li class="footer-item">
                                <a href="/Contact/Index" class="footer-item-link">
                                    Contact
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-3">
                        <h1 class="footer-heading">Follows us...</h1>
                        <h2 class="footer-heading-extra">
                            Subscribe to Newsletter
                            Stay up to date with our upcoming events, new stores and latest offers.
                        </h2>
                        <div class="send-email">
                            <input type="text" class="send-email-input" placeholder="Input your Email...">
                            <button class="send-email-btn">SEND</button>
                        </div>
                        <ul class="footer-list-icon">
                            <li class="footer-item-icon">
                                <a href="" class="footer-item-icon-link">
                                    <img src="https://vanhanhmall.com/wp-content/uploads/2018/06/icon-intergra.png"
                                         alt="" class="footer-item-icon-img">
                                </a>
                            </li>
                            <li class="footer-item-icon">
                                <a href="" class="footer-item-icon-link">
                                    <img src="https://vanhanhmall.com/wp-content/uploads/2018/06/icon-youtube.png"
                                         alt="" class="footer-item-icon-img">
                                </a>
                            </li>
                            <li class="footer-item-icon">
                                <a href="" class="footer-item-icon-link">
                                    <img src="https://vanhanhmall.com/wp-content/uploads/2018/06/icon-email.png" alt=""
                                         class="footer-item-icon-img">
                                </a>
                            </li>
                            <li class="footer-item-icon">
                                <a href="" class="footer-item-icon-link">
                                    <img src="https://vanhanhmall.com/wp-content/uploads/2018/06/icon-fb.png" alt=""
                                         class="footer-item-icon-img">
                                </a>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
            <p class="footer-text">© 2021 Ownership of FPT Aptech.</p>
        </div>
    </footer>
    <div class="centered-wrap">
        <div class="centered">
            <div class="blob-1"></div>
            <div class="blob-2"></div>
        </div>
    </div>
    @RenderSection("footerjs", required: false)
    <script>
        $(window).on("load", function () {
            $(".centered-wrap").fadeOut("slow");
        })
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({ pageLanguage: 'en' }, 'google_translate_element');
        }
    </script>
</body>
</html>
